<!-- Does not have fx:controller field because it needs to be manually set -->

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label layoutX="86.0" layoutY="210.0" text="Namn"/>
        <Label layoutX="86.0" layoutY="246.0" text="Beskrivning"/>
        <TextArea fx:id="description" editable="false" layoutX="175.0" layoutY="246.0" prefHeight="109.0"
                  prefWidth="267.0"/>
        <TextField fx:id="name" editable="false" layoutX="175.0" layoutY="206.0"/>
        <ImageView fx:id="banner" fitHeight="149.0" fitWidth="236.0" layoutX="175.0" layoutY="29.0" pickOnBounds="true"
                   preserveRatio="true"/>
    </children>
</AnchorPane>
